<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>connection_login</servlet-name>
        <servlet-class>tool.connection_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>tool.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insertionSeance</servlet-name>
        <servlet-class>tool.insertionSeance</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deco</servlet-name>
        <servlet-class>tool.deco</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>connection_login</servlet-name>
        <url-pattern>/connection_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insertionSeance</servlet-name>
        <url-pattern>/insertionSeance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deco</servlet-name>
        <url-pattern>/deco</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
